{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.66",
    "@docusaurus/plugin-content-docs": "^2.0.0-alpha.66",
    "@docusaurus/plugin-google-gtag": "^2.0.0-alpha.66",
    "@docusaurus/plugin-ideal-image": "^2.0.0-alpha.66",
    "@docusaurus/preset-classic": "2.0.0-alpha.66",
    "@docusaurus/theme-search-algolia": "^2.0.0-alpha.32",
    "@mdx-js/react": "^1.6.21",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "core-js": "^3.7.0",
    "mobx": "^6.0.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "redoc": "^2.0.0-rc.45",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@hkdobrev/run-if-changed": "^0.3.1",
    "cspell": "^5.0.1-alpha.12",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "source-map-loader": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "post-checkout": "run-if-changed",
      "post-commit": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed",
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "name": "otomi-docs",
  "private": true,
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "husky:lint-staged": "lint-staged",
    "husky:pre-commit": "lint-staged && npm run spellcheck",
    "lint": "eslint --ext js,ts .",
    "lint:all": "run-p spellcheck lint",
    "lint:fix": "eslint --ext js,ts --fix .",
    "format:fix": "prettier --write \"**/*.{json,md,yml,yaml}\"",
    "release": "docusaurus docs:version",
    "spellcheck": "cspell '/**/*.md' '/values-schema.yaml' '/README.md'",
    "start": "docusaurus start --port 3000",
    "swizzle": "docusaurus swizzle"
  },
  "version": "0.11.36"
}

{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.66",
    "@docusaurus/preset-classic": "2.0.0-alpha.66",
    "@mdx-js/react": "^1.5.8",
    "@stoplight/json-schema-viewer": "^2.12.0",
    "@stoplight/markdown-viewer": "^4.3.1",
    "@stoplight/tree-list": "^5.0.3",
    "@stoplight/ui-kit": "^2.17.2",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "core-js": "^3.7.0",
    "mobx": "^6.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "redoc": "^2.0.0-rc.45",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.66",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^16.9.56",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "cspell": "^5.0.1-alpha.8",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "source-map-loader": "^1.1.2",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "run-if-changed",
      "post-commit": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed",
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --fix"
    ]
  },
  "name": "otomi-docs",
  "private": true,
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "husky:lint-staged": "lint-staged",
    "husky:pre-commit": "lint-staged && npm run spellcheck",
    "lint": "eslint --ext js,ts .",
    "lint:all": "run-p spellcheck lint",
    "lint:fix": "eslint --ext js,ts --fix .",
    "format:fix": "prettier --write \"**/*.{json,md,yml,yaml}\"",
    "release": "docusaurus docs:version",
    "spellcheck": "cspell \"**/*\" \".**/**\"",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle"
  },
  "version": "0.11.36"
}

{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.70",
    "@docusaurus/plugin-client-redirects": "^2.0.0-alpha.70",
    "@docusaurus/plugin-content-docs": "^2.0.0-alpha.70",
    "@docusaurus/plugin-google-gtag": "^2.0.0-alpha.70",
    "@docusaurus/preset-classic": "^2.0.0-alpha.70",
    "@docusaurus/theme-search-algolia": "^2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.22",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "core-js": "^3.8.2",
    "mobx": "^6.0.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "redoc": "^2.0.0-rc.45",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@hkdobrev/run-if-changed": "^0.3.1",
    "cspell": "^5.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "source-map-loader": "^1.1.3"
  },
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "name": "otomi-docs",
  "private": true,
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "lint": "eslint --ext js,ts .",
    "lint:all": "run-p spellcheck lint",
    "lint:fix": "eslint --ext js,ts --fix .",
    "format:fix": "prettier --write \"**/*.{json,md,yml,yaml}\"",
    "release": "docusaurus docs:version",
    "serve": "docusaurus serve --port 5000",
    "spellcheck": "cspell '/**/*.md' '/README.md' --must-find-files",
    "start": "docusaurus start --port 3000",
    "swizzle": "docusaurus swizzle",
    "prepare": "husky install"
  },
  "version": "0.11.55"
}
